{
  "name": "integration-app",
  "private": true,
  "version": "1.0.0",
  "description": "Integration App",
  "scripts": {
    "start": "concurrently \"npm:start:api\" \"npm:start:ui\"",
    "postinstall": "ngcc --properties es2015 browser module main",
    "test": "ng test",
    "prettier": "prettier --write \"**/*.{cls,cmp,component,css,html,js,json,md,page,trigger,xml,yaml,yml}\"",
    "prettier:verify": "prettier --list-different \"**/*.{cls,cmp,component,css,html,js,json,md,page,trigger,xml,yaml,yml}\"",
    "ng": "ng",
    "start:ui": "ng serve --port=4202",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "start:api": "ts-node server/server.ts",
    "api:serve": "nodemon --watch server server/server.ts"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.9",
    "@angular/cli": "~12.2.9",
    "@angular/compiler-cli": "~12.2.0",
    "@prettier/plugin-xml": "^0.7.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.8.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^12.11.1",
    "@types/uniqid": "^5.3.2",
    "allure-commandline": "^2.17.2",
    "dotenv": "^16.0.0",
    "eslint": "^6.8.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.0.5",
    "prettier-plugin-apex": "^1.4.0",
    "typescript": "~4.3.5",
    "uniqid": "^5.4.0",
    "concurrently": "^7.2.2"
  },
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@veloce/api": "2.0.21",
    "@veloce/components": "2.0.21",
    "@veloce/core": "2.0.21",
    "@veloce/elements": "^1.1.0",
    "@veloce/sdk": "2.0.21",
    "axios": "^0.26.1",
    "body-parser": "^1.19.1",
    "csv": "^5.5.3",
    "express": "^4.17.2",
    "ngx-bootstrap": "^5.6.2",
    "primeng": "^12.2.3",
    "rxjs": "^7.3.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  }
}
